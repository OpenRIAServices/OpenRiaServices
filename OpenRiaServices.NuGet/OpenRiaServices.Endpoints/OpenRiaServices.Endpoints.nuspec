<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
    <id>OpenRiaServices.Endpoints</id>
    <version>4.6.0-preview1</version>
    <title>Open RIA Services SOAP and JSON Endpoints</title>
    <authors>Outercurve RIA Services</authors>
    <owners>Outercurve</owners>
    <license type="expression">Apache-2.0</license>
    <projectUrl>https://github.com/OpenRIAServices/OpenRiaServices</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      OpenRiaServices.Endpoints provides the OpenRiaServices.DomainServices.Hosting.Endpoint assembly, which serves the SOAP and JSON endpoints for Domain Services.

      The necessary web.config entries are added with this package as well.
    </description>
    <summary>Open RIA Services - SOAP and JSON Endpoints for web development</summary>
    <releaseNotes>For release notes see https://github.com/OpenRIAServices/OpenRiaServices/releases</releaseNotes>
    <copyright>2018 Outercurve Foundation</copyright>
    <language>en-US</language>
    <tags>WCF RIA Services RIAServices SOAP JSON OpenRiaServices</tags>
    <dependencies>
      <dependency id="OpenRiaServices.Server" version="$version$" />
    </dependencies>

  </metadata>
  <files>
    <file src="content\web.config.transform" target="content\web.config.transform" />
    <file src="..\..\bin\Release\net45\OpenRiaServices.DomainServices.Hosting.Endpoint.dll" target="lib\net45\OpenRiaServices.DomainServices.Hosting.Endpoint.dll" />
    <file src="..\..\bin\Release\net45\OpenRiaServices.DomainServices.Hosting.Endpoint.pdb" target="lib\net45\OpenRiaServices.DomainServices.Hosting.Endpoint.pdb" />
    <file src="..\..\bin\Release\net45\OpenRiaServices.DomainServices.Hosting.Endpoint.xml" target="lib\net45\OpenRiaServices.DomainServices.Hosting.Endpoint.xml" />
  </files>
</package>