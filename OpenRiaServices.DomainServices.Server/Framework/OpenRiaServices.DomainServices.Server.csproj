<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <TargetFramework>net45</TargetFramework>
    <OutputPath>..\..\bin\$(Configuration)\Desktop\</OutputPath>
    <DocumentationFile>..\..\bin\$(Configuration)\Desktop\OpenRiaServices.DomainServices.Server.xml</DocumentationFile>
    <DefineConstants>$(DefineConstants);SERVERFX</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="mscorlib" />
    <Reference Include="system" />
    <Reference Include="System.ComponentModel.DataAnnotations" />
    <Reference Include="System.configuration" />
    <Reference Include="System.Web" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\OpenRiaServices.DomainServices.Client\Framework\Silverlight\Data\BinaryTypeUtility.cs" Link="Data\BinaryTypeUtility.cs" />
    <Compile Include="..\..\OpenRiaServices.DomainServices.Client\Framework\Silverlight\Data\DomainException.cs" Link="Data\DomainException.cs" />
    <Compile Include="..\..\OpenRiaServices.DomainServices.Client\Framework\Silverlight\Data\DomainIdentifierAttribute.cs" Link="Data\DomainIdentifierAttribute.cs" />
    <Compile Include="..\..\OpenRiaServices.DomainServices.Client\Framework\Silverlight\Data\ExternalReferenceAttribute.cs" Link="Data\ExternalAttribute.cs" />
    <Compile Include="..\..\OpenRiaServices.DomainServices.Client\Framework\Silverlight\Data\RoundtripOriginalAttribute.cs" Link="Data\RoundtripOriginalAttribute.cs" />
    <Compile Include="..\..\OpenRiaServices.DomainServices.Client\Framework\Silverlight\Data\Serialization\KeyValue.cs" Link="Data\Serialization\KeyValue.cs" />
    <Compile Include="..\..\OpenRiaServices.DomainServices.Client\Framework\Silverlight\Data\TypeUtility.cs" Link="Data\TypeUtility.cs" />
    <Compile Include="..\..\OpenRiaServices.DomainServices.Client\Framework\Silverlight\Data\ValidationUtilities.cs" Link="Data\ValidationUtilities.cs" />
    <Compile Include="..\..\OpenRiaServices.DomainServices.Client\Framework\Silverlight\ExceptionHandlingUtility.cs" Link="Data\ExceptionHandlingUtility.cs" />
    <Compile Include="..\..\OpenRiaServices.DomainServices.Client\Framework\Silverlight\Data\ValidationResultInfo.cs" Link="Data\ValidationResultInfo.cs" />
    <Compile Include="..\..\OpenRiaServices.DomainServices.Hosting\Framework\Services\SerializationUtility.cs" Link="ApplicationServices\SerializationUtility.cs" />
    <Compile Update="Data\Resource.Designer.cs">
      <AutoGen>True</AutoGen>
      <DesignTime>True</DesignTime>
      <DependentUpon>Resource.resx</DependentUpon>
    </Compile>
    <Compile Condition=" '$(RunCodeAnalysis)' == 'true' " Update="GlobalSuppressions.cs" />
    <Compile Update="Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DesignTime>True</DesignTime>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Data\Resource.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resource.Designer.cs</LastGenOutput>
      <CustomToolNamespace>OpenRiaServices.DomainServices.Server</CustomToolNamespace>
      <SubType>Designer</SubType>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <Compile Condition=" '$(RunCodeAnalysis)' != 'true' " Remove="GlobalSuppressions.cs" />
  </ItemGroup>
</Project>