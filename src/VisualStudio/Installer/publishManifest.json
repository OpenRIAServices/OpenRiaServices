{
  "$schema": "http://json.schemastore.org/vsix-publish",
  "categories": ["coding", "web", "connected services" ], // The categories of the extension. Between 1 and 3 categories are required.
  "identity": {
    "internalName": "OpenRiaServicesTooling", // If not specified, we try to generate the name from the display name of the extension in the vsixmanifest file.
    // Required if the display name is not the actual name of the extension.
    "displayName": "Open Ria Services",
    "tags": [
      "Open Ria Services",
      "OpenRiaServices",
      "RIA",
      "OpenRia",
      "DomainServices",
      "WCF Ria Services"
    ],

  },
  "overview": "assets/overview.md", // Path to the "readme" file that gets uploaded to the Marketplace. Required.
  "priceCategory": "free", // Either "free", "trial", or "paid". Defaults to "free".
  "publisher": "OpenRiaServices", // The name of the publisher. Required.
  "private": false, // Specifies whether or not the extension should be public when uploaded. Defaults to false.
  "qna": true, // Specifies whether or not the extension should have a Q&A section. Defaults to true.
  "repo": "https://github.com/OpenRIAServices/OpenRiaServices", // Not required.
  "asetFiles": [
	{
		"pathOnDisk": "assets/images/ManageOpenRiaServicesProjectLinkContextMenu.png",
		"targetPath": "images/ManageOpenRiaServicesProjectLinkContextMenu.png"
	},
	{
		"pathOnDisk": "assets/images/ManageOpenRiaServicesProjectLink.png",
		"targetPath": "images/ManageOpenRiaServicesProjectLink.png"
	}
  ]
}